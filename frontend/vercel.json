{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://simbiosia.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "app.ateneai.com"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/clerk-proxy/(.*)",
      "destination": "https://clerk.simbiosia.com/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}